<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.z.wolfkill">

    <application
        android:name=".home.base.app.IApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".home.init.activity.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".home.main.activity.StartActivity" />
        <activity android:name=".home.main.activity.AddRoleActivity" />
        <activity
            android:name=".home.main.activity.RoleListActivity"
            android:theme="@style/DialogTheme" />
        <activity android:name=".home.main.activity.GameActivity"></activity>
    </application>

</manifest>